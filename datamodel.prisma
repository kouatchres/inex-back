type User {
  id: ID! @id @unique
  name: String!
  email: String!
  createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt

}
type Item {
  id: ID! @id @unique
  title: String!
  descrption: String!
  image: String
  largeImage: String
 createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
 
}

type Region {

id: ID! @id @unique
regName: String!
regCode: String!
division: [Division!]! @relation
createdAt: DateTime! @createdAt
updatedAt: DateTime!@updatedAt
}
type Division {

id: ID! @id @unique
divName: String!
divCode: String!
region: Region! @relation
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt

}

type SubDivision {

id: ID! @id @unique
subDivName: String!
subDivCode: String!
division: Division!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}

type Town {

id: ID! @id @unique
townName: String!
townCode: String!
subDiv: SubDivision!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}

type exanCenter {

id: ID! @id @unique
centerName: String!
centerCode: String!
town: Town!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}

type Series {

id: ID! @id @unique
seriesName: String!
seriesCode: String!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}
type Exam {

id: ID! @id @unique
examName: String!
examCode: String!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}
type Subject {

id: ID! @id @unique
subjName: String!
subjCode: String!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}
type Presence {

id: ID! @id @unique
regName: Boolean! 
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}
type ExamSession {

id: ID! @id @unique
sessionName: String!
sessionCode: String!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}

type Gender {

id: ID! @id @unique
genderName: String!
genderCode: String!
createdAt: DateTime! @createdAt
updatedAt: DateTime! @updatedAt
}


type Candidate {
id: ID! @id @unique
cand1stName: String
cand2ndName: String
cand3rdName: String
candCode: String
placeOfBirth: String
image: String
birthCertNumber: String
email: String
phoneNumb: Int
dateOfBirth: DateTime @dateOfBirth
createdAt: DateTime! @createdAt
updatedAt: DateTime!  @updatedAt
}
